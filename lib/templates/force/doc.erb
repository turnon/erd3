<h2><%= klass_name %></h2>
<% definitions[klass_name].each do |refl_type, refls| %>
  <h3><%= refl_type %></h3>
  <ul>
    <% refls.each do |refl| %>
      <li>
        <a class="select-object" data-name="<%= refl.class_name%>">
          <%= refl.stringified_definition %>
        </a>
      </li>
    <% end %>
  </ul>
<% end %>
